.well.text-center
  %h1 {{genre}}
  %p.lead Enjoy!
.row
  .col-md-5
    %paginate{collection: 'radios', path: '/genres/{{genre}}'}
    %ul.media-list.radio-thumb.with-details
      %radiothumb{'ng-repeat' => 'radio in radios', radio: 'radio', 'img-click' => 'play(radio)', 'body-click' => 'details(radio)' }
    %paginate{collection: 'radios', path: '/genres/{{genre}}'}
  .col-md-7
    %radio{radio: 'currentRadioId'}
